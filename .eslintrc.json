{
  "extends": ["react-app", "eslint:recommended", "plugin:react/recommended", "prettier"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "root": true,
  "rules": {
    "no-var": "error",
    "no-multiple-empty-lines": "error",
    "no-console": ["warn", { "allow": ["warn", "error", "info"] }],
    "eqeqeq": "error",
    "dot-notation": "error",
    "no-unused-vars": "error"
  }
}
